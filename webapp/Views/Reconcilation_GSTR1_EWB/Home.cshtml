<script type="text/javascript">

    $('#btngetMisMatch').click(function () {

        var $checkoutForm = $('#MyReport').validate({
            // Rules for form validation
            rules: {

                fromperiod: {
                    required: true
                },
                strGSTIN: {
                    required: true
                },
                toperiod: {
                    required: true
                }

            },
            messages: {

                fromperiod: {
                    required: 'Please select from date'
                },
                strGSTIN: {
                    required: 'Please select GSTIN'
                },
                toperiod: {
                    required: 'Please select to date'
                }




            }
        });
    })

</script>

@if (TempData["error"] != null)
{
    <script type="text/javascript">
        window.onload = function () {
            swal({
                title: "",
                text: "@TempData["error"]",
                type: "error",
                confirm: {
                    text: "OK",
                    value: true,
                    visible: true,
                    className: "",
                    closeModal: true,
                },
                confirmButtonColor: "#DD6B55",
                confirmButtonText: "OK",
                closeOnConfirm: true,

            }
     );
        };
    </script>
    TempData["ErrorMessage"] = null;
}
