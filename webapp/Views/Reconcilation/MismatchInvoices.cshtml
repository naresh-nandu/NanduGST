                    { "mData": "idt2a" },
                    { "mData": "val2a" },
                    { "mData": "txval2a" },
                    { "mData": "TotaltaxAmount2a" }
                ]
            });
        }
        if (actiontype == "CDNR") {
            $('#TBL_MISMATCHINVOICES').DataTable({
                destroy: true,
                "aaData": ResData,
                "aoColumns": [
                    {
                        "targets": 0,
                        "searchable": false,
                        "orderable": false,
                        "width": '2%',
                        "className": 'chkSelectAll',
                        "data": 'ctin_inum',
                        "render": function (data, type, full, meta) {
                            return '<input type="checkbox" name="InvID" id="InvID" class="chkSelectAll" value="' + data + '" />';
