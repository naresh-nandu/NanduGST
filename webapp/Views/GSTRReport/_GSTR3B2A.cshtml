
<script type="text/javascript">

    $(document).ready(function () {

        oTableStaticFlow_GSTR3B2A = $('#GSTR3B2A').DataTable({
            "lengthMenu": [[10, 50, 100, 500, 1000, -1], [10, 50, 100, 500, 1000, "All"]],
            "aoColumnDefs": [{
                'bSortable': false,
                'aTargets': [0]
            }],
        });
        $($(".sorting_asc").get(0)).removeClass("sorting_asc");
    });

</script>
<div class="table table-responsive">
    @if (ViewBag.GSTR3B2AReport != null)
    {
        <div class="container-fuild" style="background-color:white;">
            <table id="GSTR3B2A" class="table table-striped table-responsive table-bordered table-hover gridtable">
                <thead>
                    <tr>
                        <th>Description</th>
                        <th>SGST</th>
                        <th>CGST</th>
                        <th>IGST</th>
                        <th>CESS</th>
                        <th>Total</th>

                    </tr>
                </thead>


                <tbody>
                    @foreach (var item in Model.ReportMgmt)
                    {
                        <tr>
                            <td>@item.Description</td>
                            <td>@item.SGST</td>
                            <td>@item.CGST</td>
                            <td>@item.IGST</td>
                            <td>@item.CESS</td>
                            <td>@item.Total</td>
                        </tr>

                    }
                </tbody>
            </table>
        </div>
    }
</div>
