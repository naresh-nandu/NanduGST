@using System.Dynamic
@model List<System.Collections.IDictionary>
@{
    WebGrid grid = null;
    var result = new List<dynamic>();
    if (ViewBag.getEWAYBill != null)
    {
        //TempData["Data"] = Model;
        foreach (var emprow in ViewBag.getEWAYBill)
        {
            var row = (IDictionary<string, object>)new ExpandoObject();
            Dictionary<string, object> eachEmpRow = (Dictionary<string, object>)emprow;

            foreach (KeyValuePair<string, object> keyValuePair in eachEmpRow)
            {
                row.Add(keyValuePair);
            }
            result.Add(row);
        }
        grid = new WebGrid(source: result, canPage: true, canSort: true);
    }
}
<script type="text/javascript">

    $(document).ready(function () {

        oTableStaticFlow_getEWAYBill = $('#G1_getEWAYBill').DataTable({
            "lengthMenu": [[10, 50, 100, 500, 1000, -1], [10, 50, 100, 500, 1000, "All"]],
            "aoColumnDefs": [{
                'bSortable': false,
                'aTargets': [0]
            }],
        });
        $($(".sorting_asc").get(0)).removeClass("sorting_asc");

        $("#chkSelectAll_getEWAYBill").click(function () {
            var cols = oTableStaticFlow_getEWAYBill.column(0).nodes(),
                state = this.checked;

            for (var i = 0; i < cols.length; i += 1) {
                cols[i].querySelector("input[type='checkbox']").checked = state;
            }
        });

        $('.chkSelectAll_getEWAYBill').click(function () {
            var status = this.checked;
            if (status == false) {
                $("#chkSelectAll_getEWAYBill").prop("checked", false);
            }
        });

        $("#G1_getEWAYBill").on("click", ".chkSelectAll_getEWAYBill", function () {
            var status = this.checked;
            if (status == false) {
                $("#chkSelectAll_getEWAYBill").prop("checked", false);
            }
        });

        $("#btnGSTR1getEWAYBill").click(function () {
            var allVals = [];
            var selectIds = ',';
            var cols = oTableStaticFlow_getEWAYBill.column(0).nodes();
            for (var i = 0; i < cols.length; i += 1) {
                if (cols[i].querySelector("input[type='checkbox']").checked == true) {
                    var chkVal = cols[i].querySelector("input[type='checkbox']").value;
                    //allVals.push(chkVal);
                    selectIds = selectIds + chkVal + ','
                }
                else {
                    //selectIds = selectIds+'0'+ ','
                    //allVals.push(0);
                }
            }
            //console.log(selectIds);
            $("#InvIds").val();
            $("#InvIds").val(selectIds);
        });

    });

</script>

<style>
    table.dataTable thead .sorting:after {
        opacity: 0 !important;
        content: none !important;
