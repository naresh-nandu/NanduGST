@using System.Dynamic
@model List<System.Collections.IDictionary>
@{
    WebGrid grid = null;
    var result = new List<dynamic>();
    if (ViewBag.CancelList != null)
    {
        //TempData["Data"] = Model;
        foreach (var emprow in ViewBag.CancelList)
        {
            var row = (IDictionary<string, object>)new ExpandoObject();
            Dictionary<string, object> eachEmpRow = (Dictionary<string, object>)emprow;

            foreach (KeyValuePair<string, object> keyValuePair in eachEmpRow)
            {
                row.Add(keyValuePair);
            }
            result.Add(row);
        }
        grid = new WebGrid(source: result, canPage: true, canSort: true);
    }
}
<script type="text/javascript">

    $(document).ready(function () {

        oTableStaticFlow_GetCancel = $('#G1_GetCancel').DataTable({
            "lengthMenu": [[10, 50, 100, 500, 1000, -1], [10, 50, 100, 500, 1000, "All"]],
            "aoColumnDefs": [{
                'bSortable': false,
                'aTargets': [0]
            }],
        });
        $($(".sorting_asc").get(0)).removeClass("sorting_asc");


    });

</script>

<style>
    table.dataTable thead .sorting:after {
        opacity: 0 !important;
        content: none !important;
    }

    .dataTables_wrapper .dataTables_filter input {
        margin-left: 0em !important;
    }

    table.gridtable th {
        text-align: center;
        font-weight: bold;
        font-size: 14px;
        color: #3276b1;
    }

    table.gridtable td {
        text-align: center;
    }

    #chkSelectAll {
        margin-left: 4px !important;
    }

    .headerstyle {
        background-color: #cbd2d5 !important;
        color: #000 !important;
        font-weight: bold;
    }
</style>

<div class="table table-responsive">



    @if (ViewBag.CancelList != null)
    {
        if (Enumerable.Count(ViewBag.CancelList) > 0)
        {
            int i = 1;
            <div class="container-fuild" style="background-color:white;">
                <table id="G1_GetCancel" class="table table-striped table-responsive table-bordered table-hover gridtable">
                    <thead>
                        <tr>
                            <th>S.No</th>
                            <th>EWAY BillNo</th>
                            <th>EWAY BillDate</th>
                            <th>Document Number</th>
                            <th>From GSTIN</th>
                            <th>From TrdName</th>
                            <th>Total Value</th>
                            <th>Status</th>
                            <th>Error Code</th>
                            <th>Error Description</th>
                        </tr>
                    </thead>
                    <tbody>
                        @foreach (var item in result)
                        {

                           
                            <tr>
                                
                                <td>@Convert.ToInt32(i++)</td>
                                <td>@item.ewayBillNo</td>
                                <td>@item.ewayBillDate</td>
                                <td>@item.docNo</td>
                                <td>@item.fromGstin</td>
                                <td>@item.fromTrdName</td>
                                <td>@item.totalValue</td>
                                <td>@item.status</td>
                                <td>@item.CAN_errorCodes</td>
                                <td>@item.CAN_errorDescription</td>
                            </tr>
                                    }
                    </tbody>
                </table>

            </div>

                                    }

                                    else
                                    {
                                        <h3>No Data Found</h3>
                                        }
                                    }
</div>



