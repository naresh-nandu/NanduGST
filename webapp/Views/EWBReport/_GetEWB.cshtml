
<script type="text/javascript">

    $(document).ready(function () {

        oTableStaticFlow_GetEWB = $('#G1_GetEWB').DataTable({
            "lengthMenu": [[10, 50, 100, 500, 1000, -1], [10, 50, 100, 500, 1000, "All"]],
            "aoColumnDefs": [{
                'bSortable': false,
                'aTargets': [0]
            }],
        });
        $($(".sorting_asc").get(0)).removeClass("sorting_asc");


    });

</script>



<div class="table table-responsive">

    @if (ViewBag.GetEWB != null)
    {

        int i = 1;
        <div class="container-fuild" style="background-color:white;">
            <table id="G1_GetEWB" class="table table-striped table-responsive table-bordered table-hover gridtable">
                <thead>
                    <tr>

                        <th>S.No</th>
                        <th>EWAY BillNo & Date</th>
                        <th>Document Number & Date</th>
                        <th>Supply Type & Sub SupplY Type</th>
                        <th>From GSTIN Info</th>
                        <th>To GSTIN Info</th>
                        <th>Total Inv.Value</th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var item in Model.ReportMgmt)
                    {
                        <tr>

                            <td>@Convert.ToInt32(i++)</td>
                            <td>@item.ewayBillNo & @item.ewayBillDate</td>
                            <td>@item.docNo & @item.docDate</td>
                            <td>@item.supplyType & @item.subSupplyType</td>
                            <td>@item.fromGstin,@item.fromTrdName,@item.fromAddr1,@item.fromAddr2,@item.fromPlace,@item.fromPinCode</td>
                            <td>@item.toGstin,@item.toTrdName,@item.toAddr1,@item.toAddr2,@item.toPlace,@item.toPincode</td>
                            <td>@item.totalinvValue</td>

                        </tr>
                    }
                </tbody>
            </table>

        </div>


    }
</div>



