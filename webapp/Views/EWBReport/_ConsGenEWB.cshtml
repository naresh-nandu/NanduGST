
<script type="text/javascript">

    $(document).ready(function () {

        oTableStaticFlow_GenEWB = $('#G1_ConsGenEWB').DataTable({
            "lengthMenu": [[10, 50, 100, 500, 1000, -1], [10, 50, 100, 500, 1000, "All"]],
            "aoColumnDefs": [{
                'bSortable': false,
                'aTargets': [0]
            }],
        });
        $($(".sorting_asc").get(0)).removeClass("sorting_asc");


    });

</script>



<div class="table table-responsive">

    @if (ViewBag.ConsGenEWB != null)
    {

        int i = 1;
        <div class="container-fuild" style="background-color:white;">
            <table id="G1_ConsGenEWB" class="table table-striped table-responsive table-bordered table-hover gridtable">
                <thead>
                    <tr>

                        <th>S.No</th>
                        <th>Consolidate Eway Bill Details</th>
                        <th>From Details</th>
                        <th>Transporter Details</th>
                        <th>Vehicle Number</th>
                        <th>Branch Name</th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var item in Model.ConsReportMgmt)
                    {
                        <tr>

                            <td>@Convert.ToInt32(i++)</td>
                            <td>@item.Consolidate_Ewaybill_Details</td>
                            <td>@item.From_Details</td>
                            <td>@item.Transporter_Details</td>
                            <td>@item.Vehicle_No</td>
                            <td>@item.Branch_Name</td>

                        </tr>
                    }
                </tbody>
            </table>

        </div>


    }
</div>



