
<script type="text/javascript">

    $(document).ready(function () {

        oTableStaticFlow_GetEWB = $('#G1_GetEWBUserwise').DataTable({
            "lengthMenu": [[10, 50, 100, 500, 1000, -1], [10, 50, 100, 500, 1000, "All"]],
            "aoColumnDefs": [{
                'bSortable': false,
                'aTargets': [0]
            }],
        });
        $($(".sorting_asc").get(0)).removeClass("sorting_asc");


    });

</script>



<div class="table table-responsive">

    @if (ViewBag.Userwise != null)
    {

        int i = 1;
        <div class="container-fuild" style="background-color:white;">
            <table id="G1_GetEWB" class="table table-striped table-responsive table-bordered table-hover gridtable">
                <thead>
                    <tr>

                        <th>S.No</th>
                        <th>EwayBill Details</th>
                        <th>Document Details</th>
                        <th>From Details</th>
                        <th>To Details</th>
                        <th>Transporter Details</th>
                        <th>Vehicle Details</th>
                        <th>Taxable Value</th>
                        <th>Total Taxable Amount</th>
                        <th>IGST Amount</th>
                        <th>CGST Amount</th>
                        <th>SGST Amount</th>
                        <th>Cess Amount</th>
                        <th>Branch name</th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var item in Model.ReportMgmt)
                    {
                    <tr>

                        <td>@Convert.ToInt32(i++)</td>
                        <td>@Eway_Bill_Details</td>
                        <td>@Document_Details</td>
                        <td>@From_Details</td>
                        <td>@To_Details</td>
                        <td>@Transported_Details</td>
                        <td>@Vehicle_Details</td>
                        <td>@Taxable_Value</td>
                        <td>@Total_Invoice_Amount</td>
                        <td>@IGST_Amount</td>
                        <td>@CGST_Amount</td>
                        <td>@SGST_Amount</td>
                        <td>@CESS_Amount</td>
                        <td>@Branch_Name</td>

                    </tr>
                    }
                </tbody>
            </table>

        </div>


    }
</div>




