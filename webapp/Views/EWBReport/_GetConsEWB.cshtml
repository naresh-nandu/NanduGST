
<script type="text/javascript">

    $(document).ready(function () {

        oTableStaticFlow_GetConsEWB = $('#G1_GetConsEWB').DataTable({
            "lengthMenu": [[10, 50, 100, 500, 1000, -1], [10, 50, 100, 500, 1000, "All"]],
            "aoColumnDefs": [{
                'bSortable': false,
                'aTargets': [0]
            }],
        });
        $($(".sorting_asc").get(0)).removeClass("sorting_asc");


    });

</script>



<div class="table table-responsive">

    @if (ViewBag.GetConsEWB != null)
    {

        int i = 1;
        <div class="container-fuild" style="background-color:white;">
            <table id="G1_GetConsEWB" class="table table-striped table-responsive table-bordered table-hover gridtable">
                <thead>
                    <tr>

                        <th>S.No</th>
                        <th>CEWAY BillNo & Date</th>
                        <th>Trans DocNo & DocDate</th>
                        <th>Trans Mode</th>
                        <th>Vehicle No</th>
                        <th>From Place</th>
                        <th>From State</th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var item in Model.ConsReportMgmt)
                    {
                        <tr>

                            <td>@Convert.ToInt32(i++)</td>
                            <td>@item.cEwbNo & @item.cEWBDate</td>
                            <td>@item.transDocNo & @item.transDocDate</td>
                            <td>@item.transMode</td>
                            <td>@item.vehicleNo</td>
                            <td>@item.fromPlace</td>
                            <td>@item.fromState</td>

                        </tr>
                    }
                </tbody>
            </table>

        </div>


    }
</div>



