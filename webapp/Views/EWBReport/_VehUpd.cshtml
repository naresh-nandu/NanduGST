


<script type="text/javascript">

    $(document).ready(function () {

        oTableStaticFlow_GenEWB = $('#GEWBveh').DataTable({
            "lengthMenu": [[10, 50, 100, 500, 1000, -1], [10, 50, 100, 500, 1000, "All"]],
            "aoColumnDefs": [{
                'bSortable': false,
                'aTargets': [0]
            }],
        });
        $($(".sorting_asc").get(0)).removeClass("sorting_asc");


    });

</script>


<script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.6.4/angular.min.js"></script>
<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
<div class="table table-responsive">

    @if (ViewBag.VehicleUpdate != null)
    {

        int i = 1;
        <div class="container-fuild" style="background-color:white;">
            <table id="GEWBveh" class="table table-bordered">
                <thead class="blue-grey lighten-4">
                    <tr class="active">

                        <th>S.No</th>
                        <th>Eway Bill</th>
                        <th>Vehicle Number</th>
                        <th>From Place</th>
                        <th>Reason Code</th>
                        <th>Reason Rem</th>
                        <th>Transporter Document Number</th>
                        <th>Transporter Document Date</th>
                        <th>Vehicle Update Date</th>
                      
                    </tr>
                </thead>
                <tbody>
                    @foreach (var item in Model.ReportMgmt)
                    {
                        <tr>

                            <td>@Convert.ToInt32(i++)</td>
                            <td>@item.Eway_Bill_Details</td>
                            <td>@item.vehicleNo</td>
                            <td>@item.fromPlace</td>
                            <td>@item.ReasonCode</td>
                            <td>@item.ReasonRem</td>
                            <td>@item.transDocNo</td>
                            <td>@item.transDocDate</td>
                            <td>@item.vehUpdDate</td>
                            


                        </tr>
                    }
                </tbody>

            </table>

        </div>


    }
</div>



